{
  "root":true,
  "env":{
    "browser":true,
    "node":true
  },
  "extends":[
    "@nuxtjs/eslint-config-typescript",
    "plugin:nuxt/recommended",
    "@antfu/eslint-config"
  ],
  "plugins":[],
  "rules":{
    "@typescript-eslint/array-type":[
      "error",
      {
        "default":"array"
      }
    ],
    "comma-dangle":"off",
    "camelcase":"off",
    "import/named":"off",
    "no-useless-constructor":"off",
    "no-control-regex":"off",
    "no-console":"warn",
    "arrow-parens":["error", "always"],
    "brace-style":[
      "error",
      "1tbs"
    ],
    "curly":[
      "error",
      "all"
    ],
    "space-before-function-paren":[
      "error",
      {
        "anonymous":"never",
        "named":"never",
        "asyncArrow":"always"
      }
    ],
    "vue/no-multiple-template-root":"off"
  }
}
